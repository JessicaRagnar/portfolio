<!DOCTYPE html>
<html lang=sv class="no-js">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{% if meta.title %}{{ meta.title }} | {% endif %}{{ site_title }}</title>

    {% if meta.description %}
    <meta name="description" content="{{ meta.description|striptags }}" />
    {% endif %}

    {% if current_page %}
    <link rel="canonical" href="{{ current_page.url }}" />
    {% endif %}

    <link rel="icon" href="{{ base_url }}/image/logo.svg">

    {% if config.session.theme == "dark" %}
    <link rel="preload" href="{{ theme_url }}/css/style-dark.min.css" as="style" />
    <link rel="stylesheet" href="{{ theme_url }}/css/style-dark.min.css" type="text/css" />
    {% else %}
    <link rel="preload" href="{{ theme_url }}/css/style.css" as="style" />
    <link rel="stylesheet" href="{{ theme_url }}/css/style.css" type="text/css" />
    {% endif %}

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Bebas+Neue&family=Josefin+Sans:ital,wght@0,100..700;1,100..700&family=Playwrite+GB+S:ital,wght@0,100..400;1,100..400&display=swap" rel="stylesheet">
</head>
